    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "NovaFrame\\": "lib/",
            "Tests\\": "tests/"
        },
        "files": [
            "lib/function.php",
            "lib/Env/function.php",
